<!DOCTYPE module SYSTEM "module.dtd">

<module name="range_module">
  <doc>
    <description>This module contains functions to accomendate the use of single point range sensors. </description>
  </doc>
  <header>
    <file name="range_module.h"/>
  </header>
  <init fun="range_init()"/>
  <periodic fun="range_run()" freq="512" autorun="TRUE"/>
  <makefile>
    <file name="range_module.c"/>
  </makefile>
</module>

