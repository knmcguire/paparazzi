 <!DOCTYPE module SYSTEM "module.dtd">

 <module name="lisa_stereo" dir="lisa_stereo">
  <doc>
   <description>
     Get images from stereocam on Lisa/s     
   </description>
   </doc>
<header>
    <file name="lisa_stereo.h"/>
  </header>
  <periodic fun="lisa_stereo_periodic()" start="lisa_stereo_start()" stop="lisa_stereo_stop()" autorun="TRUE" freq="512"/>
  <makefile>
    <define name="USE_UART1" value="1"/>
    <define name="UART_LINK" value="uart1"/>
    <define name="UART1_BAUD" value="B921600"/>
    <file name="lisa_stereo.c"/>
  </makefile>
 </module>
